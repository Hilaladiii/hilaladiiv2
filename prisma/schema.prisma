generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Project {
  id String @id @default(auto()) @db.ObjectId @map("_id")
  imageUrl String
  name String
  demoUrl String
  githubUrl String
}

model Message {
  id String @id @default(auto()) @db.ObjectId @map("_id")
  name String
  email String 
  message String
  date DateTime? @default(now())
}
